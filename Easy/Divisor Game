## https://leetcode.com/problems/divisor-game/description/

def player_path(n):
    P = {1: False}
    for i in range(1, n + 1):
        dels = []  # 1
        for j in range(1, i):
            if i % j == 0:
                dels.append(j)
        P[i] = False

        for num in dels:
            if not P[i - num]:
                P[i] = True
    return P

def win_strategy(n):
    P = player_path(n)
    if P[n]:
        return True
    else:
        return False

print(win_strategy(2))